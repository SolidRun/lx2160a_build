From 9f1d34a5d510e590c3a9e534193ad291ad0bf8ad Mon Sep 17 00:00:00 2001
From: Jeremy Bobbin <jer@jer.cx>
Date: Sat, 5 Apr 2025 20:12:06 -0700
Subject: [PATCH] include linux delay header for pci driver

---
 drivers/pci/pcie_layerscape_gen4.c | 1 +
 drivers/pci/pcie_layerscape_rc.c   | 1 +
 2 files changed, 2 insertions(+)

diff --git a/drivers/pci/pcie_layerscape_gen4.c b/drivers/pci/pcie_layerscape_gen4.c
index eeda47470c1..94e6a92b0b9 100644
--- a/drivers/pci/pcie_layerscape_gen4.c
+++ b/drivers/pci/pcie_layerscape_gen4.c
@@ -15,6 +15,7 @@
 #include <errno.h>
 #include <malloc.h>
 #include <dm.h>
+#include <linux/delay.h>
 #include <linux/sizes.h>
 
 #include "pcie_layerscape_gen4.h"
diff --git a/drivers/pci/pcie_layerscape_rc.c b/drivers/pci/pcie_layerscape_rc.c
index c78b8efd521..ed4d1faaf4c 100644
--- a/drivers/pci/pcie_layerscape_rc.c
+++ b/drivers/pci/pcie_layerscape_rc.c
@@ -13,6 +13,7 @@
 #include <malloc.h>
 #include <dm.h>
 #include <dm/devres.h>
+#include <linux/delay.h>
 #if defined(CONFIG_FSL_LSCH2) || defined(CONFIG_FSL_LSCH3) || \
 	defined(CONFIG_ARM)
 #include <asm/arch/clock.h>
