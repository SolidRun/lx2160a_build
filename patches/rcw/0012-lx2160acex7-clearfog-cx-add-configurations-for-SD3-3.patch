+/*
+ * SerDes Protocol 1 -  18
+ * SerDes Protocol 2 -  5
+ * SerDes Protocol 3 -  3
+ *
+ * Frequencies:
+ * Core     -- 2000 MHz
+ * Platform -- 700  MHz
+ * DDR      -- 2400 MT/s
+ *
+ * Silicon 1.0
+ * Boot from AUTO
+ */
+
+#define LX_SR 1
+#define LX_BOOTSOURCE_AUTO
+
+#include <../lx2160asi/lx2160a.rcwi>
