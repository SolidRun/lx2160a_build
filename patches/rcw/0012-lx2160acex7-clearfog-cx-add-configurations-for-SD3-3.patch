+#include <../lx2160acex7/include/bootlocptr.rcwi>
+
+#if 18 == 18 || 18 == -18
+/* protocol 18 2x-25g-6x-10g convert 2x10g-25g for 4x25g on qsfp */
+#include <../lx2160asi/sd1_lane_g_h_xfi_to_25g.rcw>
